{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "fs:allow-resource-read-recursive",
    {
      "identifier": "fs:scope",
      "allow": [
        "$RESOURCE/**/*"
      ]
    }
  ]
}